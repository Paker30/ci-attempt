{
  "name": "ci-attempt",
  "version": "2.0.0",
  "description": "test automatic PR after merge",
  "main": "index.js",
  "private": true,
  "bin": {
    "create-branch": "bin/create-branch.js",
    "create-PR": "bin/create-PR.js",
    "follow-tags": "bin/follow-tags.js"
  },
  "scripts": {
    "greet": "node index.js",
    "release": "standard-version",
    "test": "echo \"There is no test in this proyect\""
